@using PaginaProyecto.Models
@model Evento
@{
    ViewBag.Title = "Eventos";
    Layout = "~/Views/Shared/_MiLayout.cshtml";
}
<h3 class="col-md-12" style="text-align:center; margin-top:10%;">Eventos</h3>

<div class="row container">
    @{
        List<Evento> listaEventos = ViewBag.ListaEventos;
        foreach (Evento oEvento in listaEventos)
        {
            <div class="jumbotron  col-md-5" style="text-align: center">
                <img src="http://www.somoscolombianos.com/wp-content/uploads/2015/05/cura-contra-el-cancer.jpg" alt="cancer" class="img-thumbnail" style="padding-left;" />
                <h5>@oEvento.NombreEvento</h5>
                <button type="button" class="btn btn-warning">@Html.ActionLink("Ver más", "Evento1", "Evento"/*, new { sucursalID = unaSucursal.ID }, null*/)</button>
            </div>
            <div col-md-2></div>
        }

    }
</div>